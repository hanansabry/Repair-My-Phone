<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.repairmyphone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.app.repairmyphone.presentation.client.MaintenanceCentersListActivity"/>
        <activity android:name="com.app.repairmyphone.presentation.client.MyRepairRequestActivity" />
        <activity android:name="com.app.repairmyphone.presentation.client.ServiceDetailsActivity" />
        <activity android:name="com.app.repairmyphone.presentation.ServiceRequestActivity" />
        <activity android:name="com.app.repairmyphone.presentation.center.RequestsActivity" />
        <activity android:name="com.app.repairmyphone.presentation.center.ServicesActivity" />
        <activity android:name="com.app.repairmyphone.presentation.center.LoginActivity" />
        <activity android:name="com.app.repairmyphone.presentation.client.RepairCarHomeActivity" />
        <activity android:name="com.app.repairmyphone.presentation.center.MaintenanceCenterRegisterationActivity" />
        <activity android:name="com.app.repairmyphone.presentation.center.MaintenanceCenterHomeActivity" />
        <activity android:name="com.app.repairmyphone.presentation.StartActivity" />
        <activity
            android:name="com.app.repairmyphone.presentation.SplashActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>